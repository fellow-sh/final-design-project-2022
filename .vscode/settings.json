    "latex-workshop.latex.tools": [
        {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-pdf",
            "-auxdir=%OUTDIR%/auxiliary",
            "-outdir=%OUTDIR%",
            "%DOC%"
        ],
        "env": {}
        },
        {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-aux-directory=%OUTDIR%/auxiliary",
            "%DOC%"
        ],
        "env": {}
        },
        {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
            "-include-directory=%OUTDIR%/auxiliary",
            "%DOCFILE%"
        ],
        "env": {}
        }
    ]
}
